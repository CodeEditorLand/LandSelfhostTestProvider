{
	"name": "vscode-selfhost-test-provider",
	"displayName": "VS Code Selfhost Test Provider",
	"version": "0.0.1",
	"private": false,
	"description": "Test provider for the VS Code project",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandSelfhostTestProvider#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandSelfhostTestProvider/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandSelfhostTestProvider.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./dist/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "selfhost-test-provider.updateSnapshot",
				"icon": "$(merge)",
				"title": "Update Snapshot"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "selfhost-test-provider.updateSnapshot",
					"when": "false"
				}
			],
			"testing/message/content": [
				{
					"command": "selfhost-test-provider.updateSnapshot",
					"when": "testMessage == isSelfhostSnapshotMessage && !testResultOutdated"
				}
			],
			"testing/message/context": [
				{
					"command": "selfhost-test-provider.updateSnapshot",
					"group": "inline@1",
					"when": "testMessage == isSelfhostSnapshotMessage && !testResultOutdated"
				}
			]
		}
	},
	"activationEvents": [
		"workspaceContains:src/vs/loader.js"
	],
	"dependencies": {
		"@jridgewell/trace-mapping": "0.3.25",
		"ansi-styles": "6.2.1",
		"js-base64": "3.7.7",
		"split2": "4.2.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.10",
		"@types/split2": "4.2.3",
		"ts-loader": "9.5.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "icon.png",
	"enabledApiProposals": [
		"testObserver"
	],
	"workspaceTrust": {
		"description": "Trust is required to execute tests in the workspace.",
		"request": "onDemand"
	}
}
